{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    { "source": "/team-form", "destination": "/index.html" },
    { "source": "/team-form/:path*", "destination": "/index.html" },
    { "source": "/((?!assets/|_next/|favicon\\.png).*)", "destination": "/index.html" }
  ]
}
